{
    "manifest_version": 2,
    "name": "BitPay Acceptance",
    "version": "1.0",
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["jquery-2.2.4.min.js", "content.js"]
      }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      },
    "browser_action": {
      "default_icon": "iconbg.png",
      "default_popup": "notaccepted.html"
    }
  }